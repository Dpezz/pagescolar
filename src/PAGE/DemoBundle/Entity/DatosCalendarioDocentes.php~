<?php

namespace PAGE\DemoBundle\Entity;

use Doctrine\ORM\Mapping as ORM;


/**
 * DatosCalendarioDocentes
 *
 * @ORM\Table()
 * @ORM\Entity()
 */
class DatosCalendarioDocentes
{
    /**
     * @var string
     *
     * @ORM\Column(name="id", type="string", length=25)
     * @ORM\Id
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="id_user", type="string", length=25)
     */
    private $id_user;

    /**
     * @var string
     *
     * @ORM\Column(name="id_docente", type="string", length=25)
     */
    private $id_docente;

    /**
     * @var string
     *
     * @ORM\Column(name="id_curso", type="string", length=25)
     */
    private $id_curso;


    /**
     * @var string
     *
     * @ORM\Column(name="id_asignatura", type="string", length=25)
     */
    private $id_asignatura;

    /**
     * @var string
     *
     * @ORM\Column(name="dia", type="string", length=50)
     */
    private $dia;

    /**
     * @var integer
     *
     * @ORM\Column(name="bloque", type="integer")
     */
    private $bloque;

    /**
     * Set id
     *
     * @param string $id
     * @return DatosCalendarioDocentes
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id
     *
     * @return string 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set id_user
     *
     * @param string $idUser
     * @return DatosCalendarioDocentes
     */
    public function setIdUser($idUser)
    {
        $this->id_user = $idUser;

        return $this;
    }

    /**
     * Get id_user
     *
     * @return string 
     */
    public function getIdUser()
    {
        return $this->id_user;
    }

    /**
     * Set id_curso
     *
     * @param string $idCurso
     * @return DatosCalendarioDocentes
     */
    public function setIdCurso($idCurso)
    {
        $this->id_curso = $idCurso;

        return $this;
    }

    /**
     * Get id_curso
     *
     * @return string 
     */
    public function getIdCurso()
    {
        return $this->id_curso;
    }

    /**
     * Set id_asignatura
     *
     * @param string $idAsignatura
     * @return DatosCalendarioDocentes
     */
    public function setIdAsignatura($idAsignatura)
    {
        $this->id_asignatura = $idAsignatura;

        return $this;
    }

    /**
     * Get id_asignatura
     *
     * @return string 
     */
    public function getIdAsignatura()
    {
        return $this->id_asignatura;
    }

    /**
     * Set id_docente
     *
     * @param string $idDocente
     * @return DatosCalendarioDocentes
     */
    public function setIdDocente($idDocente)
    {
        $this->id_docente = $idDocente;

        return $this;
    }

    /**
     * Get id_docente
     *
     * @return string 
     */
    public function getIdDocente()
    {
        return $this->id_docente;
    }

    /**
     * Set dia
     *
     * @param string $dia
     * @return DatosCalendarioDocentes
     */
    public function setDia($dia)
    {
        $this->dia = $dia;

        return $this;
    }

    /**
     * Get dia
     *
     * @return string 
     */
    public function getDia()
    {
        return $this->dia;
    }

    /**
     * Set bloque
     *
     * @param integer $bloque
     * @return DatosCalendarioDocentes
     */
    public function setBloque($bloque)
    {
        $this->bloque = $bloque;

        return $this;
    }

    /**
     * Get bloque
     *
     * @return integer 
     */
    public function getBloque()
    {
        return $this->bloque;
    }
}
