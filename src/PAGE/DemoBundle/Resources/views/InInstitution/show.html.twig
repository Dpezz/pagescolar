{% extends 'PAGEDemoBundle:Layout:layout_dashboard.html.twig' %}

{% block title "Institución"%}

{% block menu_admin_cuentas %}class="active"{% endblock %}

{% block datos %}
    <h1>Institución</h1>

    <table class="table table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Period</th>
                <td>{{ entity.period }}</td>
            </tr>
            <tr>
                <th>Mode</th>
                <td>{{ entity.mode }}</td>
            </tr>
            <tr>
                <th>Regime</th>
                <td>{{ entity.regime }}</td>
            </tr>
            <tr>
                <th>Date_start</th>
                <td>{{ entity.datestart|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Date_end</th>
                <td>{{ entity.dateend|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Createat</th>
                <td>{{ entity.createAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('ininstitution') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('ininstitution_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock %}
