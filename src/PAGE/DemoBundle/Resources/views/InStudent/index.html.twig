{% extends 'PAGEDemoBundle:Layout:layout_dashboard.html.twig' %}

{% block title "Editar Alumno"%}

{% block menu_admin_alumnos %}class="active"{% endblock %}

{% block datos %}
    <h1>Alumnos</h1>

    <table class="table table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Health</th>
                <th>Interest</th>
                <th>Antecedentes</th>
                <th>Level</th>
                <th>Origin</th>
                <th>Documents</th>
                <th>Programs</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('instudent_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.health }}</td>
                <td>{{ entity.interest }}</td>
                <td>{{ entity.antecedentes }}</td>
                <td>{{ entity.level }}</td>
                <td>{{ entity.origin }}</td>
                <td>{{ entity.documents }}</td>
                <td>{{ entity.programs }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('instudent_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('instudent_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('instudent_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
