{% extends 'PAGEDemoBundle:Layout:layout_dashboard.html.twig' %}

{% block title "Alumno"%}

{% block menu_admin_alumnos %}class="active"{% endblock %}

{% block datos %}
    <h1>Alumno</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Health</th>
                <td>{{ entity.health }}</td>
            </tr>
            <tr>
                <th>Interest</th>
                <td>{{ entity.interest }}</td>
            </tr>
            <tr>
                <th>Antecedentes</th>
                <td>{{ entity.antecedentes }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ entity.level }}</td>
            </tr>
            <tr>
                <th>Origin</th>
                <td>{{ entity.origin }}</td>
            </tr>
            <tr>
                <th>Documents</th>
                <td>{{ entity.documents }}</td>
            </tr>
            <tr>
                <th>Programs</th>
                <td>{{ entity.programs }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('instudent') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('instudent_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
