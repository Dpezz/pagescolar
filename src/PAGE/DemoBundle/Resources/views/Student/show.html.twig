{% extends 'PAGEDemoBundle:Layout:layout_dashboard.html.twig' %}

{% block title "Alumno"%}

{% block menu_admin_alumnos %}class="active"{% endblock %}

{% block datos %}
    <h1>Alumnos</h1>

    <table class="table table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Rut</th>
                <td>{{ entity.rut }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ entity.lastname }}</td>
            </tr>
            <tr>
                <th>Mlastname</th>
                <td>{{ entity.mlastname }}</td>
            </tr>
            <tr>
                <th>Birthdate</th>
                <td>{{ entity.birthdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Sex</th>
                <td>{{ entity.sex }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>Area</th>
                <td>{{ entity.area }}</td>
            </tr>
            <tr>
                <th>Commune</th>
                <td>{{ entity.commune }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ entity.phone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Etnia</th>
                <td>{{ entity.etnia }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ entity.country }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ entity.isActive }}</td>
            </tr>
            <tr>
                <th>Year_in</th>
                <td>{{ entity.yearin }}</td>
            </tr>
            <tr>
                <th>Year_end</th>
                <td>{{ entity.yearend }}</td>
            </tr>
            <tr>
                <th>Createat</th>
                <td>{{ entity.createAt|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('agent') }}">
                Apoderados
            </a>
        </li>
        <li>
            <a href="{{ path('instudentin_edit', {'id': entity.instudentin.id}) }}">
                In Student In
            </a>
        </li>
        <li>
            <a href="{{ path('instudent_edit', {'id': entity.instudent.id}) }}">
                In Student
            </a>
        </li>
        <li>
            <a href="{{ path('student') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('student_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock %}
