   {% if name_curso is not empty or null %}
   &nbsp;<i class="fa fa-angle-left"></i>&nbsp;<span class="text-mint">{{name_curso.name ~ ' ' ~ name_curso.indice}}
   {% endif %}
{% endblock %}

{% block javascripts %}
    {{parent()}}
    <script src="{{ asset('bundles/pagedemo/js/docentesController.js')}}"></script>
{% endblock %}

{% block submenu_admin_docentes %}
