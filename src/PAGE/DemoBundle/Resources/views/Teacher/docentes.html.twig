{% endblock %}

{% block datos %}
    {% embed 'PAGEDemoBundle:Layout:layout_alert.html.twig' %}
      {% block alert %}
        {{ parent() }}
      {% endblock %}
    {% endembed %}
    
	<section ng-app="docentes" ng-controller="docentes">
		{% include 'PAGEDemoBundle:Teacher:_docentes.html.twig' %}
