{% extends 'PAGEDemoBundle:Layout:layout_dashboard.html.twig' %}

{% block title "Docente"%}

{% block menu_admin_docentes %}class="active"{% endblock %}

{% block datos %}
    <h1>InTeacher</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Group</th>
                <td>{{ entity.group }}</td>
            </tr>
            <tr>
                <th>Function</th>
                <td>{{ entity.function }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ entity.level }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ entity.subject }}</td>
            </tr>
            <tr>
                <th>Certificate</th>
                <td>{{ entity.certificate }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('inteacher') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('inteacher_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
