{% extends 'PAGEDemoBundle:Layout:layout_dashboard.html.twig' %}

{% block title "Asistencias"%}

{% block active_menu_academico %}class="in"{% endblock %}

{% block subtitle %}
	 <i class="fa fa-check-circle"></i>&nbsp;Asistencia
     {% if curso %}&nbsp;<i class="fa fa-angle-left"></i>&nbsp;<span class="text-mint">{{curso}}</span>{% endif %}
     {% if fecha is not null %}&nbsp;<i class="fa fa-angle-left"></i>&nbsp;{{fecha}}{% endif %}
{% endblock %}

{% block javascripts %}
    {{parent()}}
    <script src="{{ asset('bundles/pagedemo/js/datepicker.js')}}"></script>
{% endblock %}

{% block submenu_academico_asistencias %}
    {#<li class="active"><a href="{{path('academico_asistencias')}}">&emsp;&emsp;<i class="fa fa-check-circle"></i>&nbsp;Asistencias</a></li>#}
{% endblock %}


{% block datos %}
    {% embed 'PAGEDemoBundle:Layout:layout_alert.html.twig' %}
      {% block alert %}
        {{ parent() }}
      {% endblock %}
    {% endembed %}

    
    <section>
    	{% include 'PAGEDemoBundle:Callroll:_asistencia.html.twig' %}
    	{% block academico_asistencia %}
    		{{parent()}}
    	{% endblock %}
    </section>
{% endblock %}