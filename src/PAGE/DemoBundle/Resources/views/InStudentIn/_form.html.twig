{{ form_start(form) }}
	{{ form_errors(form) }}

	{{ form_label(form.basic) }}
	<div class="well well-sm">
	{% for item in form.basic %}
		<div class="checkbox-inline">
		    <label>
		      	{{ form_widget(item) }}
		      	{{ form_label(item) }}
		    </label>
	  	</div>
  	{% endfor %}
	</div>

	{{ form_label(form.studio) }}
	<div class="well well-sm">
	{% for item in form.studio %}
		<div class="checkbox-inline">
		    <label>
		      	{{ form_widget(item) }}
		      	{{ form_label(item) }}
		    </label>
	  	</div>
  	{% endfor %}
	</div>

	{{ form_label(form.know) }}
	<div class="well well-sm">
	{% for item in form.know %}
		<div class="checkbox-inline">
		    <label>
		      	{{ form_widget(item) }}
		      	{{ form_label(item) }}
		    </label>
	  	</div>
  	{% endfor %}
	</div>

	<div class="form-group">
		{{ form_row(form.other, {'attr': {'class': 'form-control'}}) }}
	</div>

	<div class="form-group">
		{{ form_row(form.student, {'attr': {'data': entity.student.rut,'class': 'form-control','readonly':''}}) }}
	</div>
	<div class="modal-footer">
		{{ form_row(form.submit, {'attr': {'class': 'btn btn-mint'}}) }}
	</div>
{{ form_end(form) }}