{% extends 'PAGEDemoBundle:Layout:layout_dashboard.html.twig' %}

{% block title "Nuevo Alumno"%}

{% block menu_admin_alumnos %}class="active"{% endblock %}

{% block datos %}
    <h1>Alumno</h1>

    <table class="table table-hover">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Basic</th>
                <td>{{ entity.basic }}</td>
            </tr>
            <tr>
                <th>Studio</th>
                <td>{{ entity.studio }}</td>
            </tr>
            <tr>
                <th>Know</th>
                <td>{{ entity.know }}</td>
            </tr>
            <tr>
                <th>Other</th>
                <td>{{ entity.other }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('instudentin') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('instudentin_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock %}
